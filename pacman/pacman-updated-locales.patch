+# SOME DESCRIPTIVE TITLE.
+# Copyright (C) YEAR Pacman Development Team <pacman-dev@archlinux.org>
+# This file is distributed under the same license as the PACKAGE package.
+# 
+# Translators:
+# Puretech <terjemah.puretech@gmail.com>, 2013
+msgid ""
+msgstr ""
+"Project-Id-Version: manjaro-pacman\n"
+"Report-Msgid-Bugs-To: https://bugs.archlinux.org/index.php?project=3\n"
+"POT-Creation-Date: 2013-06-06 13:07+1000\n"
+"PO-Revision-Date: 2013-11-20 12:46+0000\n"
+"Last-Translator: Puretech <terjemah.puretech@gmail.com>\n"
+"Language-Team: Malay (Malaysia) (http://www.transifex.com/projects/p/manjaro-pacman/language/ms_MY/)\n"
+"MIME-Version: 1.0\n"
+"Content-Type: text/plain; charset=UTF-8\n"
+"Content-Transfer-Encoding: 8bit\n"
+"Language: ms_MY\n"
+"Plural-Forms: nplurals=1; plural=0;\n"
+
+#, c-format
+msgid "%s-%s is up to date -- skipping\n"
+msgstr "%s-%s adalah terkini -- melangkau\n"
+
+#, c-format
+msgid "%s-%s is up to date -- reinstalling\n"
+msgstr "%s-%s adalah terkini -- memasang semula\n"
+
+#, c-format
+msgid "downgrading package %s (%s => %s)\n"
+msgstr "menurun taraf pakej %s (%s => %s)\n"
+
+#, c-format
+msgid "warning given when extracting %s (%s)\n"
+msgstr "arahan diberi ketika mengekstrak %s (%s)\n"
+
+#, c-format
+msgid "could not extract %s (%s)\n"
+msgstr "tidak dapat ekstrak %s (%s)\n"
+
+#, c-format
+msgid "could not rename %s to %s (%s)\n"
+msgstr "tidak dapat menukar nama %s ke %s (%s)\n"
+
+#, c-format
+msgid ""
+"directory permissions differ on %s\n"
+"filesystem: %o  package: %o\n"
+msgstr "kebenaran direktori berbeza pada %s\nsistemfail: %o  pakej: %o\n"
+
+#, c-format
+msgid "extract: not overwriting dir with file %s\n"
+msgstr "ekstrak: tidak menulis semula dir dengan fail %s\n"
+
+#, c-format
+msgid "extract: symlink %s does not point to dir\n"
+msgstr "ekstrak: symlink %s tidak mengarah ke dir\n"
+
+#, c-format
+msgid "%s saved as %s\n"
+msgstr "%s simpan sebagai %s\n"
+
+#, c-format
+msgid "%s installed as %s\n"
+msgstr "%s pasang sebagai %s\n"
+
+#, c-format
+msgid "extracting %s as %s.pacnew\n"
+msgstr "mengekstark %s sebagai %s.pacnew\n"
+
+#, c-format
+msgid "could not get current working directory\n"
+msgstr "tidak dapat memperolehi direktori kerja semasa\n"
+
+#, c-format
+msgid "could not change directory to %s (%s)\n"
+msgstr "tidak dapat menukar direktori ke %s (%s)\n"
+
+#, c-format
+msgid "could not restore working directory (%s)\n"
+msgstr "tidak dapat mengembalikan direktori kerja semasa (%s)\n"
+
+#, c-format
+msgid "problem occurred while upgrading %s\n"
+msgstr "berlaku masalah ketika menaiktaraf %s\n"
+
+#, c-format
+msgid "problem occurred while installing %s\n"
+msgstr "berlaku masalah ketika memasang %s\n"
+
+#, c-format
+msgid "could not update database entry %s-%s\n"
+msgstr "tidak dapat mengemaskini pemasukan pangkalan data %s-%s\n"
+
+#, c-format
+msgid "could not add entry '%s' in cache\n"
+msgstr "tidak dapat menambah pemasukkan '%s' dalam cache\n"
+
+#, c-format
+msgid "error while reading file %s: %s\n"
+msgstr "ralat berlaku bila membaca fail %s: %s\n"
+
+#, c-format
+msgid "removing invalid database: %s\n"
+msgstr "membuang pangkalan data tidak sah: %s\n"
+
+#, c-format
+msgid "invalid name for database entry '%s'\n"
+msgstr "nama tidak sah untuk pemasukan pangkalan data '%s'\n"
+
+#, c-format
+msgid "duplicated database entry '%s'\n"
+msgstr "kemasukan pangkalan data duplikasi '%s'\n"
+
+#, c-format
+msgid "corrupted database entry '%s'\n"
+msgstr "kemasukan pangkalan data rosak '%s'\n"
+
+#, c-format
+msgid "could not open file %s: %s\n"
+msgstr "tidak dapat membuka fail %s: %s\n"
+
+#, c-format
+msgid "%s database is inconsistent: name mismatch on package %s\n"
+msgstr "%s pangkalan data tidak konsisten: nama tidak sepadan dengan pakej %s\n"
+
+#, c-format
+msgid "%s database is inconsistent: version mismatch on package %s\n"
+msgstr "%s pangkalan data tidak konsisten: versi tidak sepadan dengan pakej %s\n"
+
+#, c-format
+msgid "unknown validation type for package %s: %s\n"
+msgstr "jenis pengesahan tidak diketahui untuk pakej %s: %s\n"
+
+#, c-format
+msgid "could not create directory %s: %s\n"
+msgstr "tidak dapat mencipta direktori %s: %s\n"
+
+#, c-format
+msgid "could not parse package description file in %s\n"
+msgstr "tidak boleh menghurai butiran fail pakej dalam %s\n"
+
+#, c-format
+msgid "missing package name in %s\n"
+msgstr "nama pakej hilang dalam %s\n"
+
+#, c-format
+msgid "missing package version in %s\n"
+msgstr "versi pakej hilang dalam %s\n"
+
+#, c-format
+msgid "error while reading package %s: %s\n"
+msgstr "ralat berlaku bila membaca pakej %s: %s\n"
+
+#, c-format
+msgid "missing package metadata in %s\n"
+msgstr "metadata pakej hilang dalam %s\n"
+
+#, c-format
+msgid "failed to read signature file: %s\n"
+msgstr "gagal membaca tandatangan fail: %s\n"
+
+#, c-format
+msgid "required key missing from keyring\n"
+msgstr "kunci diperlukan hilang daripada keyring\n"
+
+#, c-format
+msgid "removing invalid file: %s\n"
+msgstr "membuang fail tidak sah: %s\n"
+
+#, c-format
+msgid "could not remove lock file %s\n"
+msgstr "tidak boleh membuang fail terkunci %s\n"
+
+#, c-format
+msgid "could not parse package description file '%s' from db '%s'\n"
+msgstr "tidak boleh menghurai butiran fail '%s' daripada db '%s'\n"
+
+#, c-format
+msgid "%s database is inconsistent: filename of package %s is illegal\n"
+msgstr "%s pangkalan data tidak konsisten: namafail untuk pakej %s tidak sah\n"
+
+#, c-format
+msgid "%s database is inconsistent: filename of package %s is too long\n"
+msgstr "%s pangkalan data tidak konsisten: namafail untuk pakej %s terlalu panjang\n"
+
+#, c-format
+msgid "database path is undefined\n"
+msgstr "laluan pangkalan data tidak ditetapkan\n"
+
+#, c-format
+msgid "dependency cycle detected:\n"
+msgstr "kitaran kebergantungan dikesan:\n"
+
+#, c-format
+msgid "%s will be removed after its %s dependency\n"
+msgstr "%s akan dibuang selepas %s kebergantungannya\n"
+
+#, c-format
+msgid "%s will be installed before its %s dependency\n"
+msgstr "%s akan dipasang selepas %s kebergantungannya\n"
+
+#, c-format
+msgid "ignoring package %s-%s\n"
+msgstr "abaikan pakej %s-%s\n"
+
+#, c-format
+msgid "cannot resolve \"%s\", a dependency of \"%s\"\n"
+msgstr "tidak dapat menyelesaikan \"%s\", kebergantungan untuk \"%s\"\n"
+
+#, c-format
+msgid "could not get filesystem information for %s: %s\n"
+msgstr "tidak dapat mendapatkan keterangan sistemfail untuk %s: %s\n"
+
+#, c-format
+msgid "could not open file: %s: %s\n"
+msgstr "tidak dapat membuka fail: %s: %s\n"
+
+#, c-format
+msgid "could not get filesystem information\n"
+msgstr "tidak dapat mendapatkan keterangan sistemfail\n"
+
+#, c-format
+msgid "could not determine mount point for file %s\n"
+msgstr "tidak dapat menentukan titik lekap untuk fail %s\n"
+
+#, c-format
+msgid "Partition %s too full: %jd blocks needed, %jd blocks free\n"
+msgstr "Sekatan %s terlalu penuh: %jd blok diperlukan, %jd blok kosong\n"
+
+#, c-format
+msgid "could not determine filesystem mount points\n"
+msgstr "tidak dapat menentukan titik lekap sistemfail\n"
+
+#, c-format
+msgid "could not determine cachedir mount point %s\n"
+msgstr "tidak dapat menentukan titik lekap dircache %s\n"
+
+#, c-format
+msgid "could not determine root mount point %s\n"
+msgstr "tidak dapat menentukan titik lekap root %s\n"
+
+#, c-format
+msgid "Partition %s is mounted read only\n"
+msgstr "Sekatan %s dilekapkan baca sahaja\n"
+
+#, c-format
+msgid "disk"
+msgstr "cakera"
+
+#, c-format
+msgid "failed to create temporary file for download\n"
+msgstr "gagal mencipta fail smentara untuk muat turun\n"
+
+#, c-format
+msgid "url '%s' is invalid\n"
+msgstr "url '%s' tidak sah\n"
+
+#, c-format
+msgid "failed retrieving file '%s' from %s : %s\n"
+msgstr "gagal mengambil fail '%s' dari %s : %s\n"
+
+#, c-format
+msgid "%s appears to be truncated: %jd/%jd bytes\n"
+msgstr "%s seperti terpotong: %jd/%jd bait\n"
+
+#, c-format
+msgid "failed to download %s\n"
+msgstr "gagal memuat turun %s\n"
+
+#, c-format
+msgid "out of memory!"
+msgstr "kehabisan memori!"
+
+#, c-format
+msgid "unexpected system error"
+msgstr "ralat sistem tidak dijangka"
+
+#, c-format
+msgid "permission denied"
+msgstr "keizinan dinafikan"
+
+#, c-format
+msgid "could not find or read file"
+msgstr "tidak dapat mencari atau membaca fail"
+
+#, c-format
+msgid "could not find or read directory"
+msgstr "tidak dapat mencari atau membaca direktori"
+
+#, c-format
+msgid "wrong or NULL argument passed"
+msgstr "hujah salah atau NULL dilalukan"
+
+#, c-format
+msgid "not enough free disk space"
+msgstr "ruang kosong cakera tidak mencukupi"
+
+#, c-format
+msgid "library not initialized"
+msgstr "pustaka tidak dimulakan"
+
+#, c-format
+msgid "library already initialized"
+msgstr "pustaka telah dimulakan"
+
+#, c-format
+msgid "unable to lock database"
+msgstr "tidak dapat mengunci pangkalan data"
+
+#, c-format
+msgid "could not open database"
+msgstr "tidak dapat membuka pangkalan data"
+
+#, c-format
+msgid "could not create database"
+msgstr "tidak dapat mencipta pangkalan data"
+
+#, c-format
+msgid "database not initialized"
