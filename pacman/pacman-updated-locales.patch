+#, c-format
+msgid "      --needed         do not reinstall up to date packages\n"
+msgstr "--needed без преинсталиране на актуални пакети\n"
+
+#, c-format
+msgid "  -c, --changelog      view the changelog of a package\n"
+msgstr "-c, --changelog преглед на промените за пакета\n"
+
+#, c-format
+msgid ""
+"  -d, --deps           list packages installed as dependencies [filter]\n"
+msgstr "-d, --deps списък на инсталираните пакети както зависими [filter]\n"
+
+#, c-format
+msgid "  -e, --explicit       list packages explicitly installed [filter]\n"
+msgstr "-e, --explicit списък на изрично инсталираните [filter]\n"
+
+#, c-format
